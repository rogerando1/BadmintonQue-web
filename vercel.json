{
  "version": 2,
  "builds": [
    { "src": "dist/badminton-que-web/browser/**", "use": "@vercel/static" },
    { "src": "dist/badminton-que-web/server/main.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/badminton-que-web/server/main.js"
    }
  ]
}
